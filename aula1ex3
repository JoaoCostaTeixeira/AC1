	 .text
 	.globl main
main:   
	ori	$v0,$0,5	 # a system call read_int() é
 		   		 # identificada com o número 5 
 	syscall			 # a system call read_int() é chamada 
	move	 $t0,$v0
 	ori	 $t2,$0,8	 # $t2 = 8
 	add 	$t1,$t0,$t0 	 # $t1 = x + x = 2 * x
 	sub 	$t1,$t1,$t2	 # $t1 = y = 2 * x - 8
 	
 	ori 	$v0,$0,1	#a system call print
 	move	$a0,$t1
 	syscall
 	
 	jr 	$ra 		 # fim do programa 
