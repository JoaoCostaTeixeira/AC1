# O argumento da função é passado em $a0
# O resultado é devolvido em $v0
# Sub-rotina terminal: não devem ser usados registos $sx

strlen: li $v0,0 # len = 0;
while: lb $t0,0($a0) # while(*s++ != '\0')
 	addu $a0,$a0,1 #
 	beq $t0,'\0',endw # {
 	addi $v0,$v0,1 # len++;
 	j while  # }
endw: jr $ra # 
